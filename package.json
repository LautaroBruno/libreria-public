{
  "name": "libreria-front",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@date-io/moment": "^1.3.7",
    "@material-ui/core": "4.0.1",
    "@material-ui/icons": "4.0.1",
    "@material-ui/styles": "4.0.1",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-fetch-upload": "^1.2.0",
    "apollo-upload-client": "^10.0.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron-is-dev": "^1.1.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "material-table": "1.39.0",
    "moment": "^2.24.0",
    "notistack": "^0.8.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-number-format": "^4.0.8",
    "react-scripts": "3.0.1",
    "typescript": "^3.5.2",
    "wait-on": "^3.2.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "production": "node server",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-prod": "concurrently \"cross-env BROWSER=none npm run production\" \"wait-on http://localhost:9000 && cross-env ELECTRON_IS_DEV=0 electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "build -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "./",
  "author": {
    "name": "Copisteria Centro"
  },
  "build": {
    "productName": "Copisteria Centro",
    "copyright": "Copyright Â© 2019 Copisteria Centro",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^5.0.4",
    "electron-builder": "^20.44.4"
  }
}
